{% if template contains 'article' and blog.metafields.blogfeeder.use_canonical == 'yes' %}{% assign canonical_url = article.metafields.blogfeeder.canonical %}{% endif %}
<!doctype html>
<!--[if lt IE 7]><html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]><html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]><html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if IE 9 ]><html class="ie9 no-js"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html class="no-js"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    {% if template == 'index' %}
      <title>{{ shop.name }} | {{ page_title }}</title>
    {% elsif template == '404' %}
      <title>Page Not Found | {{ shop.name }}</title>
    {% else %}
      <title>{{ page_title }} | {{ shop.name }}</title>
    {% endif %}

    {% if page_description %}
      <meta name="description" content="{{ page_description }}" />
    {% endif %}

    {{ content_for_header }}{% include 'Appify_aca' %}

    <link rel="canonical" href="{{ canonical_url }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <link rel="shortcut icon" href="{{ 'chosen-favicon.png' | asset_img_url: '32x32' }}" type="image/png">
    {% comment %} {{ 'styles.scss' | asset_url | stylesheet_tag }} {% endcomment %}
    {% comment %} {{ 'styles.scss.css' | asset_url | stylesheet_tag }} {% endcomment %}
    {{ 'style.css' | asset_url | stylesheet_tag }}
    {% if template contains 'addresses' %}
      {{ 'shopify_common.js' | shopify_asset_url | script_tag }}
    {% endif %}
  </head>
  <body>
    {% section 'header' %}
    {{ content_for_layout }}
    {% section 'footer' %}
  <!-- This line added automatically by JSON-LD for SEO. -->{% include 'jsonld-for-seo' %}
</body>
  <script src="https://code.jquery.com/jquery-3.3.1.min.js" integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=" crossorigin="anonymous"></script>
  {% comment %} {% if template == "product" %}
    {{ 'product.js' | asset_url | script_tag }}
  {% endif %}
  {% if page.handle contains 'our-avocado-oil' %}
    {{ 'parallax.js' | asset_url | script_tag }}
  {% endif %}
  {{ 'theme.js' | asset_url | script_tag }} {% endcomment %}
  {% if template == "product" %}
    <script src="{{ 'product.min.js' | asset_url }}" async></script>
  {% endif %}
  {% if page.handle contains 'our-avocado-oil' %}
    <script src="{{ 'parallax.min.js' | asset_url }}" async></script>
  {% endif %}
  <script src="{{ 'theme.min.js' | asset_url }}" defer></script>
</html>
